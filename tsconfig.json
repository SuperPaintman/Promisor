{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": false,
        "noLib": false,
        "outDir": "./bin/",
        "sourceMap": false,
        "declaration": true
    },
    "filesGlob": [
        "./development/**/*.ts"
    ],
    "exclude": [
        "node_modules",
        "bin"
    ]
}